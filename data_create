def name_data():
    name = input('Ваше имя → ')
    print('Довольно необычно)..')
    return name

def surname_data():
    surname = input('Фамилия → ')
    return surname

def phone_data():
    phone = input('Я не заискиваю. Ваш телефон → ')
    print('спасибо)')
    return phone

def addres_data():
    addres = input('И на последок, ваш адрес → ')
    print('Супер! Мы справились!')
    return addres
